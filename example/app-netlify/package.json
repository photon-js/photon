{
  "name": "app-netlify",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "netlify:deploy": "netlify deploy --filter app-netlify --no-build -d example/app-netlify/dist/client/",
    "deploy": "vite build && pnpm run netlify:deploy",
    "deploy:ssr": "vite build && rm ./dist/client/index.html && pnpm run netlify:deploy",
    "test:e2e": "playwright test",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@photonjs/netlify": "workspace:^",
    "@photonjs/core": "^0.1.14",
    "@photonjs/runtime": "^0.1.11",
    "e2e": "workspace:",
    "playwright": "^1.57.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "wrangler": "^4.51.0"
  },
  "dependencies": {
    "@universal-middleware/core": "^0.4.13",
    "awesome-framework": "0.0.0"
  }
}

{
  "name": "@photonjs/elysia",
  "version": "0.0.4",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./apply": {
      "types": "./dist/apply.d.ts",
      "development": {
        "types": "./dist/apply.dev.d.ts",
        "import": "./dist/apply.dev.js",
        "default": "./dist/apply.dev.js"
      },
      "workerd": {
        "types": "./dist/apply.edge.d.ts",
        "import": "./dist/apply.edge.js",
        "default": "./dist/apply.edge.js"
      },
      "worker": {
        "types": "./dist/apply.edge.d.ts",
        "import": "./dist/apply.edge.js",
        "default": "./dist/apply.edge.js"
      },
      "import": "./dist/apply.js",
      "default": "./dist/apply.js"
    },
    "./serve": {
      "types": "./dist/serve.d.ts",
      "deno": {
        "types": "./dist/serve.deno.d.ts",
        "import": "./dist/serve.deno.js",
        "default": "./dist/serve.deno.js"
      },
      "bun": {
        "types": "./dist/serve.bun.d.ts",
        "import": "./dist/serve.bun.js",
        "default": "./dist/serve.bun.js"
      },
      "workerd": {
        "types": "./dist/serve.d.ts",
        "import": "./dist/serve.js",
        "default": "./dist/serve.js"
      },
      "worker": {
        "types": "./dist/serve.d.ts",
        "import": "./dist/serve.js",
        "default": "./dist/serve.js"
      },
      "node": {
        "types": "./dist/serve.node.d.ts",
        "import": "./dist/serve.node.js",
        "default": "./dist/serve.node.js"
      },
      "import": "./dist/serve.js",
      "default": "./dist/serve.js"
    }
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown"
  },
  "dependencies": {
    "@photonjs/core": "workspace:^",
    "@universal-middleware/elysia": "^0.5.3"
  },
  "peerDependencies": {
    "vite": ">=7.1"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@elysiajs/node": "^1.3.0",
    "@types/bun": "^1.2.20",
    "@types/deno": "^2.3.0",
    "@types/node": "^22.17.2",
    "@universal-middleware/core": "^0.4.9",
    "elysia": "^1.3.11",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "files": [
    "dist/"
  ],
  "repository": "github:photon-js/photon",
  "license": "MIT"
}

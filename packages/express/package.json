{
  "name": "@photonjs/express",
  "version": "0.0.4",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./apply": {
      "types": "./dist/apply.d.ts",
      "development": {
        "types": "./dist/apply.dev.d.ts",
        "import": "./dist/apply.dev.js",
        "default": "./dist/apply.dev.js"
      },
      "import": "./dist/apply.js",
      "default": "./dist/apply.js"
    },
    "./serve": {
      "types": "./dist/serve.d.ts",
      "node": {
        "types": "./dist/serve.node.d.ts",
        "import": "./dist/serve.node.js",
        "default": "./dist/serve.node.js"
      },
      "import": "./dist/serve.js",
      "default": "./dist/serve.js"
    }
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown"
  },
  "dependencies": {
    "@photonjs/core": "workspace:^",
    "@universal-middleware/express": "^0.4.19"
  },
  "peerDependencies": {
    "vite": ">=7.1"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "@universal-middleware/core": "^0.4.10",
    "express": "^5.1.0",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "files": [
    "dist/"
  ],
  "repository": "github:photon-js/photon",
  "license": "MIT"
}
